	<!-- data 2  Formato YYYYMMDD -->
	<simpleType name="st_data2">
		<restriction base="string">
			<maxLength value="8"/>
			<pattern value="[0-9]{4}[0-9]{2}[0-9]{2}"/>
		</restriction>
	</simpleType>
	<!-- data 1  Formato YYYY/MM/DDHH:MI:SS~GG -->
	<simpleType name="st_data1">
		<restriction base="string">
			<minLength value="1"/>
			<maxLength value="21"/>
			<pattern value="[0-9]{4}/[0-9]{2}/[0-9]{2}[0-9]{2}:[0-9]{2}:[0-9]{2}[+-][0-9]{2}"/>
		</restriction>
	</simpleType>
