		</restriction>
	</simpleType>
	<simpleType name="st_hora">
		<restriction base="string">
			<maxLength value="8"/>
			<pattern value="[0-9]{2}:[0-9]{2}:[0-9]{2}"/>
		</restriction>
	</simpleType>
	<simpleType name="st_cdCID">
		<restriction base="string">
			<pattern value="[A-Z][0-9]{2}[0-9]?"/>
			<!-- Composto por uma letra e dois digitos de preenchimento obrigatorio e adicionalmente um digito opcional-->
		</restriction>
