	<simpleType name="st_Excecao">
		<restriction base="string">
			<enumeration value="0"/>
			<enumeration value="C"/>
			<enumeration value="I"/>
			<enumeration value="L"/>
			<enumeration value="O"/>
			<enumeration value="E"/>
			<enumeration value="J"/>
			<enumeration value="A"/>
			<!-- 
				 0 = Atendimento Normal, não exigiu autorização	
				 C = Atendimento autorizado em CO, fora das regras de intercambio
				 I = Nota glosada reapresentada pelo prestador
